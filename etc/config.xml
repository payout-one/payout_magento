<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <payout>
                <active>1</active>
                <title>Pay Via Payout</title>
                <payment_action>Authorization</payment_action>
                <test>1</test>
                <sandbox_mode>1</sandbox_mode>
                <model>Payout\Payment\Model\Payout</model>
                <can_authorize_vault>1</can_authorize_vault>
                <is_gateway>0</is_gateway>
                <can_order>1</can_order>
                <can_use_checkout>1</can_use_checkout>
                <can_capture>1</can_capture>
                <can_void>1</can_void>
                <can_refund>0</can_refund>
                <can_use_for_multishipping>1</can_use_for_multishipping>
                <internal_payout_test_override>0</internal_payout_test_override>
            </payout>
        </payment>
    </default>
</config>
